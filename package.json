{
  "name": "ljve",
  "version": "0.4.20140730+porting",
  "description": "Live Javascript Virtualized Environment",
  "author": "Marco Minetti <marco.minetti@novetica.org>",
  "contributors": [],
  "bin": {
    "ljve": "./main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcominetti/ljve.io"
  },
  "bugs": {
    "url": "https://github.com/marcominetti/ljve.io/issues"
  },
  "keywords": [
    "ljve",
    "ljve.io"
  ],
  "dependencies": {
    "mocha": "*",
    "chai": "*",
    "blanket": "*",
    "coveralls": "*",
    "mocha-lcov-reporter": "*",
    "yargs": "*",
    "cli-color": "*",
    "ws": "*",
    "xmlhttprequest": "*",
    "webworker-threads": "*",
    "fs-extra": "*",
    "minimatch": "*",
    "underscore": "*",
    "memwatch": "*",
    "chokidar": "*",
    "esprima": "*",
    "universal-analytics": "*",
    "sloc": "*",
    "webkit-devtools-agent": "https://github.com/marcominetti/node-webkit-agent/tarball/master",
    "webkit-devtools-agent-frontend-console": "https://github.com/marcominetti/webkit-devtools-agent-frontend-console/tarball/master",
    "webkit-devtools-agent-frontend-inspector": "https://github.com/marcominetti/webkit-devtools-agent-frontend-inspector/tarball/master",
    "webkit-devtools-agent-frontend-profiler": "https://github.com/marcominetti/webkit-devtools-agent-frontend-profiler/tarball/master"
  },
  "scripts": {
    "postinstall": "NODE_ENV=development node main.js coverage | sed 1d > stats/coverage.html; wget `node_modules/.bin/sloc --format json core | node tools/sloc.js` -O stats/comments.badge.png; wget https://codeclimate.com/github/marcominetti/ljve.io.png -O stats/climate.badge.png; wget https://coveralls.io/repos/marcominetti/ljve.io/badge.png?branch=master -O stats/coverage.badge.png;",
    "test": "if [ -f \".travis.yml\" ]; then NODE_ENV=test node main.js coverage | sed 1d | ./node_modules/coveralls/bin/coveralls.js; else NODE_ENV=development node main.js test; fi;"
  },
  "gypfile": false,
  "preferGlobal": true,
  "private": false,
  "analyze": true,
  "license": "AGPLv3"
}
