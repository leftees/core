{
  "preset": "google",
  "esnext": true,
  "excludeFiles": [
    "bin/*",
    "data/*",
    "lib/*",
    "log/*",
    "project/*",
    "tmp/*",
    "**/build/*",
    "**/cache/*",
    "**/dist/*",
    "**/node_modules/*",
    "app/*/*",
    "config/*/*",
    "core/_old/**"
  ],
  "maximumLineLength": 120,
  "requireCamelCaseOrUpperCaseIdentifiers": false,
  "maxErrors": -1
}